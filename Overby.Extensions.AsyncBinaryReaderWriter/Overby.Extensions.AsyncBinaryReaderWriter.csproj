<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>
      net6.0	  
    </TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <Authors>Ronnie Overby</Authors>
    <Company />
    <Product />
    <Description>Asynchronous versions of the BinaryReader and BinaryWriter classes.</Description>
    <Copyright>Ronnie Overby 2017</Copyright>
    <RepositoryUrl>https://github.com/ronnieoverby/AsyncBinaryReaderWriter</RepositoryUrl>
    <PackageProjectUrl>https://github.com/ronnieoverby/AsyncBinaryReaderWriter</PackageProjectUrl>
    <PackageVersion>1.0.0</PackageVersion>
    <PackageLicenseUrl>https://raw.githubusercontent.com/ronnieoverby/AsyncBinaryReaderWriter/release/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO" Version="4.3.0" />
    <PackageReference Include="System.Memory" Version="4.5.5" />
  </ItemGroup>

</Project>
